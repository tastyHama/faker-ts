    "typescript": "^4.5.5",
    "typescript-json-schema": "^0.53.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/faker": "^4.1.5",
    "@types/koa": "^2.0.48",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.20.45",
    "chai": "^4.2.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "mocha": "^9.2.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "type-fest": "^2.11.2"
  },
  "version": "0.0.2"
}
