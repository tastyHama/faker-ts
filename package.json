{
  "name": "faker-ts",
  "main": "dist/faker-ts.js",
  "typings": "dist/faker-ts.d.ts",
  "bin": "dist/bin/faker-ts.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "npm run build && mocha -t 5000 --require source-map-support/register dist/tests",
